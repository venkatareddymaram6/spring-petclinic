pipeline{
    agent { label 'JDK11-MVN3.8.4' }
    triggers { 
        upstream(upstreamProjects: 'starter project', threshold: hudson.model.Result.SUCCESS)
         cron('30 23 * * 1-5') }
    stages{
        stage('download sources') {
            steps{
            git branch: 'declarative', url: 'https://github.com/venkatareddymaram6/spring-petclinic.git'
        }
            }
        stage('build') {
        steps {
            sh '/usr/local/apache-maven-3.8.5/bin/mvn clean package'
        }
    }
}

}